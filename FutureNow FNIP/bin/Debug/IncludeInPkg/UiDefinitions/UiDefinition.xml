<?xml version="1.0" encoding="utf-8" ?>
<uidefinition xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
              xsi:noNamespaceSchemaLocation=" https://prd-use-rad-assets.azurewebsites.net/ExtensionsSchemaDefinition.xsd">
	<version ver="3.0"/>
	<tile icon="{AllLightsIcon}" navigation="show:MainPage" action="command:AllOn" showinhomepage="#false" showinroompage="#true"/>
	<layouts>
		<layout id="MainPage" title="^MainPageTitle" isdefaultlayout="#true">
			<controls>
				<toggleslider id="Light1" label="{Name1value}" secondarylabel="{Percent1value}" togglevalue="{Light1value}" slidervalue="{Slider1value}"/>
				<toggleslider id="Light2" label="{Name2value}" secondarylabel="{Percent2value}" togglevalue="{Light2value}" slidervalue="{Slider2value}"/>
				<toggleslider id="Light3" label="{Name3value}" secondarylabel="{Percent3value}" togglevalue="{Light3value}" slidervalue="{Slider3value}"/>
				<toggleslider id="Light4" label="{Name4value}" secondarylabel="{Percent4value}" togglevalue="{Light4value}" slidervalue="{Slider4value}"/>
				<toggleslider id="Light5" label="{Name5value}" secondarylabel="{Percent5value}" togglevalue="{Light5value}" slidervalue="{Slider5value}"/>
				<toggleslider id="Light6" label="{Name6value}" secondarylabel="{Percent6value}" togglevalue="{Light6value}" slidervalue="{Slider6value}"/>
			</controls>
		</layout>
	</layouts>
	<alerts/>
</uidefinition>